<%- partial ('partials/statusmessage.ejs') %>

<h2>Edit Musician</h2>
<form action="/update" method="POST" class="form-horizontal">
  <div class="form-group">
    <label for="musicianID" class="col-sm-2 control-label">Musician ID</label>
    <div class="col-sm-10">
      <input type="number" class="form-control" id="musicianID" name="musicianID" placeholder="Musician ID">
    </div>
  </div>
  <div class="form-group">
    <label for="firstName" class="col-sm-2 control-label">First Name</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="firstName" name="firstName" placeholder="First Name">
    </div>
  </div>
  <div class="form-group">
    <label for="lastName" class="col-sm-2 control-label">First Name</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="lastName" name="lastName"  placeholder="Last Name">
    </div>
  </div>
  <div class="form-group">
    <label for="typeOfMusic" class="col-sm-2 control-label">Type of Music</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="typeOfMusic" name="typeOfMusic" placeholder="Type of Music">
    </div>
  </div>
  <div class="form-group">
    <label for="homeCity" class="col-sm-2 control-label">Home City</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="homeCity" name="homeCity" placeholder="Home City">
    </div>
  </div>
  <div class="form-group">
    <label for="homeState" class="col-sm-2 control-label">Home State</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="homeState" name="homeState" placeholder="Home State">
    </div>
  </div>
  <div class="form-group">
    <label for="phone" class="col-sm-2 control-label">Phone</label>
    <div class="col-sm-10">
      <input type="tel" class="form-control" id="phone" name="phone" placeholder="###-###-####">
    </div>
  </div>
  <div class="form-group">
    <label for="phone" class="col-sm-2 control-label">Salary</label>
    <div class="col-sm-10">
      <input type="number" class="form-control" id="salary" name="salary" placeholder="Salary">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">Edit Musician</button>
    </div>
  </div>
</form>